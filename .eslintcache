ates.map((state,i) => {\n            return(\n                <Dropdown.Item eventKey={i + 1} onClick={(event)=>{props.onStateSelected(state);\n                setState(state);\n                }} >{state}</Dropdown.Item>\n            )\n            })}\n        </Dropdown.Menu>\n    </Dropdown>\n    { state? <div>Selected State: {state}</div> : null }\n    </div>\n    )\n    };\n\nexport default VaccineSearchState;",{"ruleId":"59","severity":1,"message":"60","line":3,"column":8,"nodeType":"61","messageId":"62","endLine":3,"endColumn":16},{"ruleId":"59","severity":1,"message":"63","line":5,"column":10,"nodeType":"61","messageId":"62","endLine":5,"endColumn":18},{"ruleId":"59","severity":1,"message":"64","line":5,"column":20,"nodeType":"61","messageId":"62","endLine":5,"endColumn":34},{"ruleId":"59","severity":1,"message":"65","line":3,"column":10,"nodeType":"61","messageId":"62","endLine":3,"endColumn":16},{"ruleId":"59","severity":1,"message":"66","line":5,"column":8,"nodeType":"61","messageId":"62","endLine":5,"endColumn":17},{"ruleId":"59","severity":1,"message":"67","line":6,"column":8,"nodeType":"61","messageId":"62","endLine":6,"endColumn":19},{"ruleId":"68","severity":1,"message":"69","line":13,"column":5,"nodeType":"70","endLine":20,"endColumn":6},{"ruleId":"71","severity":1,"message":"72","line":1,"column":17,"nodeType":"61","messageId":"62","endLine":1,"endColumn":21},{"ruleId":"71","severity":1,"message":"73","line":1,"column":23,"nodeType":"61","messageId":"62","endLine":1,"endColumn":31},{"ruleId":"71","severity":1,"message":"74","line":1,"column":33,"nodeType":"61","messageId":"62","endLine":1,"endColumn":42},{"ruleId":"75","severity":1,"message":"76","line":42,"column":49,"nodeType":"77","messageId":"78","endLine":42,"endColumn":51},{"ruleId":"71","severity":1,"message":"79","line":50,"column":35,"nodeType":"61","messageId":"62","endLine":50,"endColumn":37},{"ruleId":"59","severity":1,"message":"80","line":3,"column":10,"nodeType":"61","messageId":"62